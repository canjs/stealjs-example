{
  "name": "stealjs-example",
  "version": "1.0.0",
  "description": "An example app for CanJS, using StealJS.",
  "author": "Bitovi",
  "license": "MIT",
  "homepage": "https://github.com/canjs/stealjs-example.js#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/canjs/stealjs-example.js.git"
  },
  "bugs": {
    "url": "https://github.com/canjs/stealjs-example.js/issues"
  },
  "keywords": [
    "CanJS",
    "DoneJS",
    "StealJS"
  ],
  "main": "main.js",
  "scripts": {
    "build": "steal-tools build",
    "clean": "rm -rf dist/bundles dist/steal.production.js",
    "start": "http-server -c-1"
  },
  "dependencies": {
    "can": "^6.0.0"
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "steal": "^2.2.4",
    "steal-tools": "^2.0.0"
  },
  "steal": {
    "babelOptions": {
      "plugins": [
        "transform-class-properties"
      ]
    },
    "plugins": [
      "can"
    ]
  }
}
